<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.elefantitoverde">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ElefantitoVerde">
        <activity
            android:name=".ModificarProducto"
            android:parentActivityName=".MainActivity">
        </activity>
        <activity
            android:name=".DetalleProducto"
            android:parentActivityName=".ListViewProductos"/>
        <activity
            android:name=".ListViewProductos"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".AgregarProducto"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".AgregarCategoria"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.ElefantitoVerde.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>